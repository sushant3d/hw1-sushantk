<?xml version="1.0" encoding="UTF-8"?><typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>Sentence</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <imports>
    <import location="NamedEntityDescriptor.xml"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.cmu.lti.type.Sentence</name>
      <description>Stores the sentence information from document collection</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>SourceID</name>
          <description>Stores the document collection ID</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>SourceText</name>
          <description>Stores the document collection text</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>NamedEntityList</name>
          <description>Stores the list of Named Entities extracted in given source</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.lti.type.NamedEntity</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
